<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:ValueConverters="clr-namespace:TosReactiveUI.Commons.Converters"
             xmlns:local="clr-namespace:TosReactiveUI"
             x:Class="TosReactiveUI.App">
    <Application.Resources>
        <ResourceDictionary>
            <ValueConverters:TypeToBackgroundColorValueConverter x:Key="TypeToBackgroundColorValueConverter"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

